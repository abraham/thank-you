<section class="links mdc-card__supporting-text">
  Citations:
  <% if deed.tweet? %>
    <%= link_to 'Twitter', deed.tweet.url.to_s, target: '_blank' %>
  <% end %>
  <%= render links %>
  <% if current_user && (current_user.edit?(deed) || current_user.admin?) %>
    <span><%= link_to 'Add link', new_deed_link_path(deed) %></span>
  <% end %>
</section>
